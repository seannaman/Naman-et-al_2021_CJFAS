file = {:Users/seannaman/Library/Application Support/Mendeley Desktop/Downloaded/Jakober, McMahon, Thurow - 2000 - Diel habitat partitioning by bull charr and cutthroat trout during fall and winter in Rocky Mountain s.pdf:pdf},
isbn = {0378-1909, 0378-1909},
issn = {03781909},
journal = {Environmental Biology of Fishes},
keywords = {Diel behavior,Habitat use,Oncorhynchus clarki,Salmonids,Salvelinus confluentus,Winter ecology},
number = {1},
